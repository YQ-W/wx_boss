<view id="header">拉钩网</view>
<view class="item" wx:for="{{success.content.data.page.result}}" wx:key="{{key}}">
  <image class='logo' src="{{'http://www.lgstatic.com/' + item.companyLogo}}"></image>
  <view class="item-desc">
      <text class='item-title'>{{item.companyName}}</text>
      <view class='item-info'>{{item.positionName}}
          <text class="item-salary">
            {{item.salary}}
          </text>
      </view>
      <text class='item-time'>{{item.createTime}}</text>
  </view>
  
</view>
